{
  "name": "studio-desktop-app",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/studio-desktop-app/src",
  "projectType": "application",
  "implicitDependencies": ["studio-desktop-app-ui"],
  "targets": {
    "build": {
      "executor": "nx-electron:build",
      "dependsOn": ["^build"],
      "outputs": ["{options.outputPath}"],
      "cache": true,
      "options": {
        "outputPath": "{workspaceRoot}/dist/{projectRoot}",
        "main": "{projectRoot}/src/main.ts",
        "tsConfig": "{projectRoot}/tsconfig.app.json",
        "assets": ["{projectRoot}/src/assets"]
      },
      "configurations": {
        "production": {
          "optimization": true,
          "extractLicenses": true,
          "inspect": false,
          "fileReplacements": [
            {
              "replace": "{projectRoot}/src/environments/environment.ts",
              "with": "{projectRoot}/src/environments/environment.prod.ts"
            }
          ]
        }
      }
    },
    "serve": {
      "executor": "nx-electron:execute",
      "dependsOn": ["^serve"],
      "continuous": true,
      "options": {
        "buildTarget": "studio-desktop-app:build",
        "inspect": false
      }
    },
    "package": {
      "executor": "nx-electron:package",
      "dependsOn": ["build"],
      "outputs": ["{options.outputPath}"],
      "cache": true,
      "options": {
        "frontendProject": "studio-desktop-app-ui",
        "sourcePath": "dist/apps",
        "makerOptionsPath": "{projectRoot}/electron-builder.json",
        "name": "studio-desktop-app",
        "prepackageOnly": true
      },
      "defaultConfiguration": "windows",
      "configurations": {
        "windows": {
          "outputPath": "dist/packages/studio-desktop-app/windows",
          "platform": ["windows"],
          "arch": "x64"
        },
        "windows-arm": {
          "outputPath": "dist/packages/studio-desktop-app/windows-arm",
          "platform": ["windows"],
          "arch": "arm64"
        },
        "mac": {
          "outputPath": "dist/packages/studio-desktop-app/mac",
          "platform": ["mac"],
          "arch": "x64"
        },
        "mac-arm": {
          "outputPath": "dist/packages/studio-desktop-app/mac-arm",
          "platform": ["mac"],
          "arch": "arm64"
        }
      }
    },
    "make": {
      "executor": "nx-electron:make",
      "dependsOn": ["build"],
      "outputs": ["{options.outputPath}"],
      "cache": true,
      "options": {
        "frontendProject": "studio-desktop-app-ui",
        "makerOptionsPath": "{projectRoot}/electron-builder.json",
        "name": "studio-desktop-app",
        "outputPath": "dist/executables/studio-desktop-app",
        "sourcePath": "dist/apps"
      },
      "defaultConfiguration": "windows",
      "configurations": {
        "windows": {
          "platform": ["windows"],
          "arch": "x64"
        },
        "windows-arm": {
          "platform": ["windows"],
          "arch": "arm64"
        },
        "mac": {
          "platform": ["mac"],
          "arch": "x64"
        },
        "mac-arm": {
          "platform": ["mac"],
          "arch": "arm64"
        }
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": ["{options.outputFile}"],
      "cache": true,
      "options": {
        "lintFilePatterns": ["{projectRoot}/**/*.ts"]
      }
    }
  }
}
